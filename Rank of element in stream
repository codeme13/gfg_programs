// C++ program to find rank of an 
// element in a stream. 
#include <bits/stdc++.h> 
using namespace std; 

// Driver code 
int main() 
{ 
	int a[] = {5, 1, 14, 4, 15, 9, 7, 20, 11}; 
	int key = 20; 
	int arraySize = sizeof(a)/sizeof(a[0]); 
	int count = 0; 
	for(int i = 0; i < arraySize; i++) 
	{ 
		if(a[i] <= key) 
		{ 
			count += 1; 
		} 
	} 
	cout << "Rank of " << key << " in stream is: "
			<< count-1 << endl; 
	return 0; 
} 

