// C++ Program to implement 
// the above approach 
#include <bits/stdc++.h> 
using namespace std; 

// Function to construct a string 
// having N*N non-palindromic substrings 
void createString(int N) 
{ 
	for (int i = 0; i < N; i++) { 
		cout << 'a'; 
	} 
	for (int i = 0; i < N; i++) { 
		cout << 'b'; 
	} 
} 

// Driver Code 
int main() 
{ 
	int N = 4; 

	createString(N); 
	return 0; 
} 
