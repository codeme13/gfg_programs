
#include <bits/stdc++.h>
using namespace std;

int main() {
int t;
cin>>t;
while(t--)
{
int n;cin>>n;
unsigned long long i,j,flag=1,a[n],b[n];
for( i=0;i<n;i++) {cin>>a[i];
}
for( j=0;j<n;j++) { cin>>b[j];
}
sort(a,a+n);
sort(b,b+n);

for(int i=0;i<n;i++) 
{ 
    if(a[i]!=b[i]) 
    {
        flag=0;
        break;
    }
}
   cout<<flag<<endl;
}
return 0;
    
}
