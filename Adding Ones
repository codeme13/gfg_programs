#include <bits/stdc++.h>
using namespace std ;
int main()
{
int t ; cin>>t ; while(t--)
{
int n , m ; cin >> n >> m ;
int var2[n+1] = {0} ; int var1[m] ;
for(int i = 0 ; i<m ;i++)
{
    cin>> var1[i] ; }
for( int j = 0 ; j < m ; j++)
{
int krishna = var1[j] ;
while(krishna < n+1 )
{ var2[krishna]++ ; krishna++ ; }
}
for(int i = 1 ; i < n+1 ; i++)
cout << var2[i] << " " ;
cout << endl ;
}
return 0 ;
}
