#include<iostream>
using namespace std;
int main()
{
int t;
cin>>t;
while(t--)
{
    int n;
    cin>>n;
    int arr[n];
    for(int i=0; i<n; i++)
    {
        cin>>arr[i];
    }
    int length=0;
    int maxlen=0;
    for(int i=0; i<n; i++)
    {
        if(arr[i]>=0)
        length++;
        else 
        length=0;
        if(length>maxlen)
        maxlen=length;
    }
    cout<<maxlen<<endl; 
    }
    
}
