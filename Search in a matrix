#include<bits/stdc++.h>
using namespace std;

int main() 
{
    int t;
    cin>>t;
    while(t--)
    {
        int m,n,i,j,x;
        cin>>m>>n;
        map<int,int>mp;
        for(i=0;i<m;i++) 
        {
            for(j=0;j<n;j++)
            {
                cin>>x;
                mp[x]=1;
            }
        }

    int k;
    cin>>k;
        if(mp[k]==1)
        {
            cout<<1<<endl; 
        }
        else 
        {
            cout<<0<<endl;
        } 
        mp.clear();
        }
    return 0;
}
