#include<bits/stdc++.h>
using namespace std;
int main()
{
    int s; cin>>s;
int arr[s];
for(int i=0;i<s;i++) cin>>arr[i];
int m=INT_MAX;
for(int i=0;i<s;i++)
    if(m>arr[i])
        m=arr[i];
    int sm=INT_MAX;
for(int i=0;i<s;i++)
    if(sm>arr[i] & arr[i]!=m)
        sm=arr[i];
cout<<sm;
return 0;
}
