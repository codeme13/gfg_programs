#include <bits/stdc++.h> 
using namespace std; 

int main() 
{ 
	int n;
	cout<<"Enter the number of elements ";
	cin>>n;
	int arr[n];
	for(int i=0;i<n;i++)
		cin>>arr[i];
	int k;
	cout<<"Enter number for search ";
	cin>>k;
	cout<<"number of count of "<<k<<" is "<< count(arr, arr + n, k);
	return 0; 
} 
